{
  "name": "ddm-dropdown",
  "authors": [
    "Joshua Stoutenburg <jstoutenburg@deseretdigital.com>",
    "Russell Ahlstrom <rahlstrom@deseretdigital.com>"
  ],
  "description": "Dropdown component for DDM projects.",
  "main": "dist/**/*",
  "moduleType": [
    "globals"
  ],
  "keywords": [
    "ui",
    "dropdown",
    "ddm"
  ],
  "license": "MIT",
  "homepage": "http://github.com/deseretdigital-ui/ddm-dropdown",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "src"
  ],
  "overrides": {
    "react": {
      "main": [
        "react-with-addons.js",
        "JSXTransformer.js"
      ]
    },
    "es5-shim": {
      "main": [
        "es5-shim.js",
        "es5-sham.js"
      ]
    }
  },
  "dependencies": {
    "jquery": ">=1.7 <3",
    "react": "~0.12",
    "es5-shim": "~4.0.3"
  }
}
